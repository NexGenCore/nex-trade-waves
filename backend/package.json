{
  "name": "nex-trade-wave-backend",
  "version": "1.0.0",
  "private": true,
  "description": "NestJS backend for Nex Trade Wave - DeFi trading and learning platform",
  "license": "MIT",
  "author": "Nex Trade Wave Team",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "debug": "node --inspect-brk -r tsconfig-paths/register dist/main.js",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "10.4.20",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/microservices": "10.4.20",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "7.1.17",
    "@nestjs/typeorm": "10.0.2",
    "@nestjs/websockets": "10.4.20",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "mysql2": "^3.6.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^22",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
